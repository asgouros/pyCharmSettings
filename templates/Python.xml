<templateSet group="Python">
  <template name="ipdb" value="import ipdb; ipdb.set_trace(context=15)" shortcut="ENTER" description="ipdb breakpoint" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="@nu" value="@not_unittest" description="not-unit-test decorator" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="prr" value="print('-------------', repr($VAR$))" description="Print-repr" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="pdb" value="import pdb; pdb.set_trace()" shortcut="ENTER" description="pdb breakpoint" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="ppr" value="from pprint import pformat&#10;print('\n---{1}:\n{0}\n'.format(&#10;    pformat($VAR$, sort_dicts=False), '$VAR$'))&#10;" description="pretty-print" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
</templateSet>